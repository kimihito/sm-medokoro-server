<% @movie_providers.each do |provider| %>
	<div id="<%= provider %>">
		<h1><%= provider %></h1>
		<ul class="row movies">
			<% eval("@#{provider}_movies").each do |movie| %>
				<%= render partial: "layouts/movie_list", locals: {movie: movie} %>
			<% end %>
		</ul>
		<p class="text-right">
			<%= provider %>で本日人気の動画
		</p>
	</div>
<% end %>